    <gyro site="right_knee_mimic" name="right_knee_gyro"/>
    <accelerometer site="right_knee_mimic" name="right_knee_accel"/>
    <framequat objtype="site" objname="right_knee_mimic" name="right_knee_quat"/>
    <framelinvel objtype="site" objname="right_knee_mimic" name="right_knee_linvel"/>
    <gyro site="right_foot_mimic" name="right_foot_gyro"/>
    <accelerometer site="right_foot_mimic" name="right_foot_accel"/>
    <framequat objtype="site" objname="right_foot_mimic" name="right_foot_quat"/>
    <framelinvel objtype="site" objname="right_foot_mimic" name="right_foot_linvel"/>
    <framezaxis objtype="site" objname="right_foot_mimic" name="right_foot_upvector"/>
    <!-- Left leg sensors -->
    <gyro site="left_hip_mimic" name="left_hip_gyro"/>
    <accelerometer site="left_hip_mimic" name="left_hip_accel"/>
    <framequat objtype="site" objname="left_hip_mimic" name="left_hip_quat"/>
