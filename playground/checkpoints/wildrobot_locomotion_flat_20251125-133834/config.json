{
  "config_file": "quick.yaml",
  "overrides": {},
  "final_config": {
    "env": {
      "terrain": "flat",
      "ctrl_dt": 0.02,
      "sim_dt": 0.002,
      "terminal_state_params": {
        "min_height": 0.2,
        "max_height": 0.7
      },
      "reward_params": {
        "tracking_w_exp_xy": 15.0,
        "tracking_w_lin_xy": 5.0,
        "tracking_w_exp_yaw": 4.0,
        "tracking_w_lin_yaw": 1.0,
        "tracking_sigma": 0.25,
        "z_vel_coeff": 2.0,
        "roll_pitch_vel_coeff": 0.08,
        "roll_pitch_pos_coeff": 0.3,
        "nominal_joint_pos_coeff": 0.005,
        "joint_position_limit_coeff": 5.0,
        "joint_vel_coeff": "5e-5",
        "joint_acc_coeff": "2e-5",
        "action_rate_coeff": 0.02,
        "joint_torque_coeff": "2e-7",
        "energy_coeff": "2e-5"
      }
    },
    "training": {
      "num_timesteps": 100000,
      "num_evals": 2,
      "episode_length": 200,
      "seed": 0
    },
    "ppo": {
      "num_envs": 256,
      "num_eval_envs": 32,
      "batch_size": 128,
      "unroll_length": 5,
      "num_minibatches": 2,
      "num_updates_per_batch": 2,
      "learning_rate": 0.0003,
      "entropy_cost": 0.01,
      "discounting": 0.97,
      "reward_scaling": 0.1,
      "max_grad_norm": 1.0,
      "clipping_epsilon": 0.2,
      "normalize_observations": true,
      "action_repeat": 1
    },
    "network": {
      "policy_hidden_layers": [
        128,
        128
      ],
      "value_hidden_layers": [
        128,
        128
      ]
    },
    "logging": {
      "use_wandb": false,
      "wandb_project": "wildrobot",
      "wandb_entity": null
    },
    "rendering": {
      "render_videos": false,
      "render_height": 480,
      "render_width": 640
    },
    "checkpointing": {
      "save_interval": 1000000,
      "keep_checkpoints": 3
    },
    "validation": {
      "metrics": {
        "reward": "eval/episode_reward",
        "episode_length": "eval/avg_episode_length"
      },
      "thresholds": {
        "reward_tolerance": 0.1,
        "length_tolerance": 0.1,
        "determinism_threshold": 0.01
      }
    }
  }
}